{
	"version": 2,
	"functions": {
		"server.js": {
			"maxDuration": 30
		}
	},
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/server.js",
			"headers": {
				"Access-Control-Allow-Origin": "*",
				"Access-Control-Allow-Methods": "GET,PUT,POST,DELETE,OPTIONS",
				"Access-Control-Allow-Headers": "Content-Type, Authorization, X-Requested-With"
			}
		}
	]
}
