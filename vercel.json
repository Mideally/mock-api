{
	"version": 2,
	"builds": [
		{
			"src": "server.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/",
			"dest": "/server.js"
		},
		{
			"src": "/companies(.*)",
			"dest": "/server.js"
		},
		{
			"src": "/moments(.*)",
			"dest": "/server.js"
		},
		{
			"src": "/drops(.*)",
			"dest": "/server.js"
		},
		{
			"src": "/(.*)",
			"dest": "/server.js"
		}
	]
}
